"---------------------------------------------------------------------------
" neetular's .gvimrc
"---------------------------------------------------------------------------
"colorscheme hybrid
"colorscheme jellybeans
"-------
colorscheme aldmeris

set guifont=Ricty\ Discord:h18

set guioptions&
set guioptions-=e
set guioptions-=T
set guioptions+=l

set columns=999
set lines=99
set antialias
set iminsert=0
set imsearch=0

highlight Cursor guifg=bg guibg=#9dd8a7
highlight CursorIM guifg=bg guibg=#c951ab

highlight Pmenu ctermbg=8 guibg=#606060
highlight PmenuSel ctermbg=1 guibg=#000000 guifg=#1f82cd
highlight PmenuSbar ctermbg=0 guibg=#d6d6d6

highlight Search guifg=NONE guibg=#07678b

